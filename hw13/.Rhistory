sec_report$composite_scores
# Bootstrapped path coefficients: t-values, 95% CI
boot_pls <- bootstrap_model(sec_pls, nboot = 1000)
summary(boot_pls)
sec_cbsem <- estimate_cbsem(data = sec, as.reflective(sec_mm))
plot(sec_cbsem)
