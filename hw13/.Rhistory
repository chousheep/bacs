sec_report <- summary(sec_pls)
sec_report$weights
cat("loadings:","\n" sec_report$loadings)
cat("loadings:","\n",sec_report$loadings)
cat("loadings:","\n",sec_report$loadings)
#sec_report$weights
sec_report$total_effects
# Weights and loadings of composites
sec_report <- summary(sec_pls)
#sec_report$weights
?sec_report$composite_scores
